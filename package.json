{
  "name": "nicefox-graphdb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "cp hooks/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "pnpm --filter nicefox-graphdb-server dev",
    "build": "pnpm -r build"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}
